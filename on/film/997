    <h3 data-bind="visible: selectedVisibleseason()">Part of the <a href="/on/season/130" class="season" itemprop="superEvent" itemscope itemtype="http://schema.org/Event" data-bind="attr:{href:selectedLinkseason},text:selectedLabelseason">Girl Gang Film Season</a></h3>

    

    <h3><time class="startDateTime" itemprop="startDate" datetime="2012-09-13 19:30:00+01:00" data-bind="attr:{datetime:startDateTime()},text:displayStart()">7:30 p.m. Thursday 13 September 2012</time></h3>

    <div itemscope itemtype="http://schema.org/Movie">
        <meta itemprop="name" content="From The Back Of The Room" data-bind="attr:{content:title}">
        <meta itemprop="copyrightyear" content="+ skype Q&A with Amy Oden" data-bind="attr:{content:year}">
        <meta itemprop="image" content="
                /media/cache/12/55/1255dd6a8290080ba493e33e591c7054.jpg">
        <table class="table table-condensed event-table">
            <tbody>
            <tr>
                <th>Director:</th>
                <td><span itemprop="director" itemscope itemtype="http://schema.org/Person"><span class="director" itemprop="name" data-bind="text:director">Amy Oden</span></span></td>
            </tr>
