    <div id="event-285" class="eventtype editthis" itemscope itemtype="http://schema.org/Event" data-modeltype="Event" data-modelid="285" data-apiobjecturl="/api/1/event/285/">
    <meta itemprop="description" content="The festival of G.B. - Gender identity themed art, film and music festival. Part of the 2010 Northern pride celebrations." data-bind="attr: { content: summary }"/>
    <meta itemprop="url" content="/on/event/285"/>
