    <div id="season-130" class="eventtype editthis" itemscope itemtype="http://schema.org/Event" data-modeltype="Season" data-modelid="130" data-apiobjecturl="/api/1/season/130/">
    <meta itemprop="description" content="A season of awesome films about radical girl gangs" data-bind="attr: { content: summary }"/>
    <meta itemprop="url" content="/on/season/130"/>
    <meta itemprop="duration" content="" data-bind="attr: { content: isolength }"/>
